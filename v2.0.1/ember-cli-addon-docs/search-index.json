]},"template:docs/packages/ember-cli-testdouble-qunit":{"position":[[10,10],[51,10],[159,10],[209,10],[237,11],[429,11],[491,13]]},"template:docs/packages/testdouble-qunit":{"position":[[0,10],[131,11],[178,10],[228,10]]}},"keywords":{}}],["tests/test",{"_index":65,"title":{},"text":{"template:docs/packages/ember-cli-testdouble-qunit":{"position":[[344,10],[374,10]]}},"keywords":{}}],["through",{"_index":58,"title":{},"text":{"template:docs/packages/ember-cli-testdouble-qunit":{"position":[[225,7]]}},"keywords":{}}],["time",{"_index":15,"title":{},"text":{"template:docs/examples/call-count":{"position":[[300,6],[434,6]]}},"keywords":{}}],["true",{"_index":20,"title":{},"text":{"template:docs/examples/call-count":{"position":[[461,4]]}},"keywords":{}}],["twicetest('stub",{"_index":14,"title":{},"text":{"template:docs/examples/call-count":{"position":[[171,15]]}},"keywords":{}}],["typeof",{"_index":31,"title":{},"text":{"template:docs/examples/stubbing":{"position":[[52,6]]}},"keywords":{}}],["up",{"_index":73,"title":{},"text":{"template:docs/packages/testdouble-qunit":{"position":[[68,2],[267,3]]}},"keywords":{}}],["us",{"_index":76,"title":{},"text":{"template:docs/packages/testdouble-qunit":{"position":[[125,5]]}},"keywords":{}}],["verifi",{"_index":0,"title":{"template:docs/examples/call-count":{"position":[[0,9]]}},"text":{"template:docs/examples/call-count":{"position":[[0,9],[498,9]]},"template:docs/index":{"position":[[17,6],[121,6]]}},"keywords":{}}],["yarn",{"_index":77,"title":{},"text":{"template:docs/packages/testdouble-qunit":{"position":[[161,4],[166,4]]}},"keywords":{}}]],"pipeline":["stemmer"]},"documents":{"template:docs/examples/call-count":{"id":"template:docs/examples/call-count","type":"template","title":"Verifying Call Count","text":"Verifying Call CountFunction is calledtest('stub is never called', assert => { const stub = td.function(); stub('foo'); assert.verify(stub('foo')); }); Function is called twicetest('stub is never called', assert => { const stub = td.function(); stub('foo'); stub('foo'); assert.verify(stub('foo'), { times: 2 }); }); Function is never calledtest('stub is never called', assert => { const stub = td.function(); assert.verify(stub(), { times: 0, ignoreExtraArgs: true }); }); For more information on verifying invocations, consult the testdouble documentation.","route":"docs.examples.call-count","keywords":[]},"template:docs/examples/stubbing":{"id":"template:docs/examples/stubbing","type":"template","title":"Stubbing Results","text":"Stubbing Resultsfunction returnsNumber(cb) { return typeof cb() === 'number'; } test('the callback returns a number', assert => { const cb = td.function(); td.when(cb()).thenReturn(42); assert.ok(returnsNumber(cb)); }); For more information on stubbing results, consult the testdouble documentation.","route":"docs.examples.stubbing","keywords":[]},"template:docs/index":{"id":"template:docs/index","type":"template","title":"testdouble-qunit","text":"testdouble-qunit Verify testdouble stubs with QUnit This library adds a assert.verify method to QUnit that allows you to verify that your stubs are called correctly. test('the stub is called', function(assert) { const stub = td.function(); stub('foo'); assert.verify(stub('foo')); }); For installation information, go here: ","route":"docs.index","keywords":[]},"template:docs/packages/ember-cli-testdouble-qunit":{"id":"template:docs/packages/ember-cli-testdouble-qunit","type":"template","title":"Installation","text":"ember-cli-testdouble-qunitThis Ember addon exposes testdouble-qunit so that you can easily consume it in your application. Installationember install ember-cli-testdouble-qunit This will also install ember-cli-testdouble (and through it, testdouble) for you automatically. Then, inject the new assertion by adding the following anywhere in your tests/test-helper.js file: // tests/test-helper.js import injectVerifyAssertion from 'testdouble-qunit'; import QUnit from 'qunit'; import td from 'testdouble'; injectVerifyAssertion(QUnit, td); ","route":"docs.packages.ember-cli-testdouble-qunit","keywords":[]},"template:docs/packages/testdouble-qunit":{"id":"template:docs/packages/testdouble-qunit","type":"template","title":"Installation","text":"testdouble-qunitThis library exposes a function that will set QUnit up with some additional assertions that are helpful when using testdouble. Installation# For Yarn yarn add -D testdouble-qunit # For NPM npm install --save-dev testdouble-qunit You also have to \"set up\" the integration by running the following before your tests. const injectVerifyAssertion = require('testdouble-qunit'); const QUnit = require('qunit'); const td = require('testdouble'); injectVerifyAssertion(QUnit, td); ","route":"docs.packages.testdouble-qunit","keywords":[]},"template:docs":{"id":"template:docs","type":"template","title":null,"text":" ","route":"docs","keywords":[]},"template:application-tailwind":{"id":"template:application-tailwind","type":"template","title":"Your Tailwind styles","text":" Your Tailwind styles A reference for every generated class in your app. ","route":"application-tailwind","keywords":[]},"template:docs/api/item":{"id":"template:docs/api/item","type":"template","title":null,"text":" ","route":"docs.api.item","keywords":[]}}}
