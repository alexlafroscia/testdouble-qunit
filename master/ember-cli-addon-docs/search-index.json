e:docs/packages/ember-cli-testdouble-qunit":{"position":[[229,10]]}},"keywords":{}}],["time",{"_index":12,"title":{},"text":{"template:docs/examples/call-count":{"position":[[144,6],[316,6]]}},"keywords":{}}],["twicetest('stub",{"_index":15,"title":{},"text":{"template:docs/examples/call-count":{"position":[[180,15]]}},"keywords":{}}],["up",{"_index":51,"title":{},"text":{"template:docs/packages/testdouble-qunit":{"position":[[68,2],[206,3]]}},"keywords":{}}],["us",{"_index":55,"title":{},"text":{"template:docs/packages/testdouble-qunit":{"position":[[125,5]]}},"keywords":{}}],["verifi",{"_index":0,"title":{"template:docs/examples/call-count":{"position":[[0,9]]}},"text":{"template:docs/examples/call-count":{"position":[[0,9]]},"template:docs/index":{"position":[[17,6],[121,6]]}},"keywords":{}}]],"pipeline":["stemmer"]},"documents":{"template:docs/examples/call-count":{"id":"template:docs/examples/call-count","type":"template","title":"Verifying Call Count","text":"Verifying Call CountFunction is never calledtest('stub is never called', function(assert) { const stub = td.function(); assert.verify(stub(), { times: 0 }); }); Function is called twicetest('stub is never called', function(assert) { const stub = td.function(); stub('foo'); stub('foo'); assert.verify(stub('foo'), { times: 2 }); }); ","route":"docs.examples.call-count","keywords":[]},"template:docs/index":{"id":"template:docs/index","type":"template","title":"testdouble-qunit","text":"testdouble-qunit Verify testdouble stubs with QUnit This library adds a assert.verify method to QUnit that allows you to verify that your stubs are called correctly. test('the stub is called', function(assert) { const stub = td.function(); stub('foo'); assert.verify(stub('foo')); }); For installation information, go here: ","route":"docs.index","keywords":[]},"template:docs/packages/ember-cli-testdouble-qunit":{"id":"template:docs/packages/ember-cli-testdouble-qunit","type":"template","title":"Installation","text":"ember-cli-testdouble-qunitThis Ember addon exposes testdouble-qunit so that you can easily consume it in your application. Installationember install ember-cli-testdouble ember-cli-testdouble-qunit Then, import the module in your tests/test-helper.js file to install the integration: import testdoubleQunit from 'testdouble-qunit'; import QUnit from 'qunit'; import td from 'testdouble'; testdoubleQunit(QUnit, td); ","route":"docs.packages.ember-cli-testdouble-qunit","keywords":[]},"template:docs/packages/testdouble-qunit":{"id":"template:docs/packages/testdouble-qunit","type":"template","title":"Installation","text":"testdouble-qunitThis library exposes a function that will set QUnit up with some additional assertions that are helpful when using testdouble. Installationyarn add -D testdouble-qunit You also have to \"set up\" the integration by running the following before your tests. const testdoubleQunit = require('testdouble-qunit'); const QUnit = require('qunit'); const td = require('testdouble'); testdoubleQunit(QUnit, td); ","route":"docs.packages.testdouble-qunit","keywords":[]},"template:docs":{"id":"template:docs","type":"template","title":null,"text":" ","route":"docs","keywords":[]},"template:application-tailwind":{"id":"template:application-tailwind","type":"template","title":"Your Tailwind styles","text":" Your Tailwind styles A reference for every generated class in your app. ","route":"application-tailwind","keywords":[]},"template:docs/api/item":{"id":"template:docs/api/item","type":"template","title":null,"text":" ","route":"docs.api.item","keywords":[]}}}
